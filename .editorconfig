root = true

[*]
end_of_line = lf
charset = utf-8

[*.js,*.ts,*.tsx,*.jsx,*.json] 
indent_style = space
indent_size = 2
insert_final_newline = true

[*.cshtml,*.html,*.css,*.scss,*.cs,*.md]
indent_style = space
indent_size = 4
insert_final_newline = true

[*.xml, *.txt]
insert_final_newline = false
