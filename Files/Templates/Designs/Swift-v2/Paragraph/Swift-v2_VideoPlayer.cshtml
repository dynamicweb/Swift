@inherits Dynamicweb.Rendering.ViewModelTemplate<Dynamicweb.Frontend.ParagraphViewModel>
@using System.IO
@using Dynamicweb.Frontend

@if(Model.Item.TryGetVideo("VideoSource", out VideoViewModel video)) {
	@RenderPartial("Components/VideoPlayer.cshtml", video, 
		new Dictionary<string, object>() {
			{"VideoName", Model.Item.GetString("VideoName")},
			{"VideoDescription", Model.Item.GetString("VideoDescription")}
		})
}
