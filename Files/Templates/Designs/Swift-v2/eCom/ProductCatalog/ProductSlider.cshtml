@inherits ViewModelTemplate<ProductListViewModel>
@using Dynamicweb.Rendering
@using Dynamicweb.Ecommerce.ProductCatalog

@{
	string partialToRender = !string.IsNullOrEmpty(Dynamicweb.Context.Current.Request.QueryString.Get("ProductListPartial")) ? Dynamicweb.Context.Current.Request.QueryString.Get("ProductListPartial") : "ProductSliderComponent";
}

@RenderPartial($"eCom/ProductCatalog/{partialToRender}.cshtml", Model)
