{
	"plugins": "table wordcount",
	"menubar": "",
	"toolbar": "code | undo redo | bold italic underline strikethrough | styleselect | numlist bullist | removeformat",
	"quickbars_selection_toolbar": "bold italic | h1 h2 h3",
	"branding": false,
	"style_formats": [
		{
			"title": "Muted text",
			"inline": "span",
			"styles": { "opacity": "0.75" }
		},
		{
			"title": "Headings",
			"items": [
				{
					"title": "Heading 1",
					"format": "h1"
				},
				{
					"title": "Heading 2",
					"format": "h2"
				},
				{
					"title": "Heading 3",
					"format": "h3"
				}
			]
		},
		{
			"title": "Inline",
			"items": [
				{
					"title": "Bold",
					"format": "bold"
				},
				{
					"title": "Italic",
					"format": "italic"
				},
				{
					"title": "Underline",
					"format": "underline"
				}
			]
		},
		{
			"title": "Blocks",
			"items": [
				{
					"title": "Paragraph",
					"format": "p"
				}
			]
		}
	],
	"fontsize_formats": ""
}
